<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="20sp" android:left="5sp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0" >
                <shape android:shape="rectangle">
                    <solid android:color="@color/colorThemeDark" />
                </shape>
        </rotate>
    </item>
    <item android:left="15sp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/colorThemeDark" />
            <corners android:radius="5sp" />
        </shape>
    </item>
</layer-list>