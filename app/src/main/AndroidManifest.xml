<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="utar.edu.mad">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/black"
        android:label="@string/app_name"
        android:roundIcon="@drawable/black"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Message"></activity>
        <activity android:name=".NewFriend" />
        <activity android:name=".SongUpload"></activity>
        <activity android:name=".Chat" />
        <activity android:name=".Friend" />
        <activity android:name=".SongList" />
        <activity android:name=".UpdateUser" />
        <activity android:name=".ShowProfile" />
        <activity android:name=".CreateProfile" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:theme="@style/Login" />
        <activity
            android:name=".login"
            android:theme="@style/Login" />
    </application>

</manifest>